

<!-- <%=projectinfo%> -->

<div class="row ">
    <div class="col-sm-8 w-75 mx-auto text-center">
      <div class="card">
        <div class="card-body">
          <h5 class="card-title"><%=projectinfo.name%></h5>
          <p class="card-text"><%=projectinfo.description%></p>
          <p class="card-text"><%=projectinfo.author%></p>
          <a href="/issue/createissue/<%=projectinfo.id%>" class="btn btn-primary">Create Issue</a>
        </div>
      </div>
    </div>
    
  </div>
 
  <% for(i of projectinfo.issues){%>
    <div class="row ">
        <div class="col-sm-8 w-75 mx-auto text-center">
          <div class="card">
            <div class="card-body">
              <h5 class="card-title"><%=i.title%></h5>
              <p class="card-text"><%=i.description%></p>
              <p class="card-text"><%=i.author%></p>
              <p class="card-text"><%=i.lable%></p>
              <a href="/issue/deleteissue/<%=i.id%>" class="btn btn-primary">Delete Issue</a>
            </div>
          </div>
        </div>
        
      </div>
<%}%>